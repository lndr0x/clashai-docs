					{
						"group": "Chat UI API Setup",
						"pages": [
							"chatui/introduction",
							"chatui/liberchat",
							"chatui/venusai",
							"chatui/chatkit",
							"chatui/msty"
						]
					}
				],
				"icon": "compass"
			},
			{
				"tab": "API Reference",
				"groups": [
					{
						"group": "API Documentation",
						"pages": ["api-reference/introduction"]
					},
					{
						"group": "Endpoint Examples",
						"pages": [
							"api-reference/endpoint/get",
